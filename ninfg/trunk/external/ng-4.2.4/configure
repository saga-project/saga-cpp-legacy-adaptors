void test_va(int c, int n, ...)
{
    va_list va;

    va_start(va, n);
    test_va2(c, n, va);
    va_end(va);
}

void test_va2(int c, int n, va_list va)
{
    void (*fp)();
    int i, j;

    for (i = 0; i < n; i++) {
        fp = va_arg(va, void(*)());
        for (j = 0; j < c; j++) {
            (*fp)();
        }
    }
}

void cb_increment()
{
    counter++;
}
EOF
