	<entity name="Extension" ldapname="GLUE2Extension">

		<!-- no relatedEntity as we do not have a concrete Entity object-->

		<!-- Extension -->
		<attribute name="LocalID" type="LocalID_t"
			ldapname="GLUE2ExtensionLocalId" />
		<attribute name="Key" type="string"
			ldapname="GLUE2ExtensionKey" />
