[saga.adaptors.aws_job]
  name               = aws_job

[saga.adaptors.aws_job.preferences]
  cloud_names        = ec2 eucalyptus nimbus
  java_home          = @JAVA_LOCATION@
  ec2_home           = @AWS_LOCATION@
  ec2_keystore       = ${HOME}/.ec2_keys/

[saga.adaptors.aws_job.preferences.ec2]
  ec2_proxy          = /tmp/saga.aws_private_ec2.pm
  ec2_key            = $[saga.adaptors.aws_job.preferences.ec2_keystore]/ec2-key.pem
  ec2_cert           = $[saga.adaptors.aws_job.preferences.ec2_keystore]/ec2-cert.pem
  ec2_url            = https://ec2.amazonaws.com/
  ec2_keypair        = ec2-saga-keypair-ec2
  ec2_image_id       = ami-b7b858de

[saga.adaptors.aws_job.preferences.eucalyptus]
  ec2_proxy          = /tmp/saga.aws_private_eucalyptus.pm
  ec2_key            = $[saga.adaptors.aws_job.preferences.ec2_keystore]/eucalyptus-key.pem
  ec2_cert           = $[saga.adaptors.aws_job.preferences.ec2_keystore]/eucalyptus-cert.pem
  ec2_url            = http://mayhem9.cs.ucsb.edu:8773/services/Eucalyptus
  ec2_keypair        = ec2-saga-keypair-eucalyptus
  ec2_image_id       = emi-0D05022C

[saga.adaptors.aws_job.preferences.nimbus]
  ec2_proxy          = /tmp/saga.aws_private_nimbus.pm
  ec2_key            = $[saga.adaptors.aws_job.preferences.ec2_keystore]/nimbus-key.pem
  ec2_cert           = $[saga.adaptors.aws_job.preferences.ec2_keystore]/nimbus-cert.pem
  ec2_url            = https://tp-vm1.ci.uchicago.edu:8445/wsrf/services/ElasticNimbusService
  ec2_keypair        = ec2-saga-keypair-nimbus
  ec2_ssh_pubkey     = ssh-dss  bzfmerzk@cocos
  ec2_image_id       = hello-cloud

